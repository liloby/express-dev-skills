<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Oby's Developer Skills</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
    <h1 class="gradient-text">My Developer Skills</h1>
    <ul class="skill-list">
      <% skills.forEach(function(skill) { %>
        <li class="completion">
          <a href="/skills/<%= skill.id %>"><%= skill.skill %></a>
        </li>
      <% }); %>
    </ul>
    <a class="redirect" href="/"><-Home Page</a>
  </body>
</html>